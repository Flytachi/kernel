{
    "name": "flytachi/kernel",
    "description": "Extra Framework kernel",
    "type": "library",
    "license": "MIT",
    "version": "5.7.2",
    "keywords": [
        "Extra Kernel",
        "extra kernel",
        "framework kernel",
        "kernel"
    ],
    "autoload": {
        "psr-4": {
            "Flytachi\\Kernel\\": "/",
            "Flytachi\\Kernel\\Src\\": "src/",
            "Flytachi\\Kernel\\Console\\": "console/"
        },
        "files": [
            "function/Dependencies.php",
            "function/Tags.php"
        ]
    },
    "authors": [
        {
            "name": "Flytachi",
            "email": "jasur.rakhmatov03@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "flytachi/db-mapping": "~1.2.0",
        "flytachi/file-store": "1.0.1",
        "monolog/monolog": "3.9.0",
        "vlucas/phpdotenv": "v5.6.1",
        "ext-simplexml": "*",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-redis": "*",
        "ext-posix": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "@stable"
    }
}
